{
  "name": "poshlostios-client",
  "version": "1.0.0",
  "description": "PoshlostiOS as desktop ENVIRONMENT",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --linux"
  },
  "repository": "https://github.com/Forbirdden/PoshlostiOS-client",
  "author": {
    "name": "Forbirdden",
    "email": "@forbirdden."
  },
  "license": "CC0-1.0",
  "build": {
    "appId": "com.forbirdden.poshlostios",
    "productName": "PoshlostiOS",
    "asar": false,
    "publish": {
      "provider": "github",
      "releaseType": "draft"
    },
    "linux": {
      "target": [
        {
          "target": "apk",
          "arch": ["x64", "arm64"]
        }
      ]
    },
  "devDependencies": {
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12"
  }
}
